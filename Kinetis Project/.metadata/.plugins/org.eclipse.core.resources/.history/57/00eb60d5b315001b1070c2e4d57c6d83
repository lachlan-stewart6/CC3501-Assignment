#ifndef SOURCES_MAIN_H_
#define SOURCES_MAIN_H_

#define QUEUE_LENGTH 2

typedef struct {
	int msg_id ;
	byte acc[6];
	byte gyro[6];
	byte mag[6];
} Message ;

typedef struct {
	LWMSGQ_STRUCT hearder;
	Message msgs [QUEUE_LENGTH];
}MessageQueue;




#endif /* SOURCES_MAIN_H_ */
